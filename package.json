{
    "name": "@arcadia-moon/push-receiver",
    "version": "4.3.0",
    "description": "A module to subscribe to GCM/FCM and receive notifications within a node process.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "package*",
        "*.md"
    ],
    "scripts": {
        "protos:convert": "npx pbjs -t static-module --force-long --wrap commonjs -o ./src/protos.js ./src/protos/*.proto",
        "protos": "npx pbts -o ./src/protos.d.ts ./src/protos.js",
        "protos:build": "npm run protos:convert && npm run protos",
        "protos:copy": "cp ./src/protos.* ./dist/",
        "build": "npm run protos:build && npx tsc && npm run protos:copy",
        "prepublish": "npm run build",
        "lint": "npx eslint src --ext .ts",
        "precommit": "npm run lint && npm run build",
        "prepare": "npm run prepublish"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/arcadia-moon/push-receiver.git"
    },
    "keywords": [
        "push",
        "service",
        "fcm",
        "gcm",
        "notifications",
        "node",
        "electron",
        "receiver"
    ],
    "author": "Eneris & MatthieuLemoine",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/arcadia-moon/push-receiver/issues"
    },
    "homepage": "https://github.com/arcadia-moon/push-receiver#readme",
    "devDependencies": {
        "@types/node": "^22.8.2",
        "@typescript-eslint/eslint-plugin": "^8.12.1",
        "@typescript-eslint/parser": "^8.12.1",
        "eslint": "^9.13.0",
        "protobufjs-cli": "^1.1.3",
        "typescript": "^5.6.3"
    },
    "dependencies": {
        "http_ece": "^1.2.1",
        "jsonwebtoken": "^9.0.2",
        "long": "^5.2.3",
        "protobufjs": "^7.4.0"
    },
    "engines": {
        "node": ">=20"
    }
}
